{
  "name": "prlx",
  "version": "0.1.3",
  "description": "Small, performance optimized tool for parallax image backgrounds.",
  "keywords": [
    "parallax",
    "scroll"
  ],
  "homepage": "https://github.com/luukdv/parallax-background",
  "bugs": {
    "url": "https://github.com/luukdv/parallax-background/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Luuk de Vlieger"
  },
  "main": "dist/prlx.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/luukdv/parallax-background.git"
  },
  "scripts": {
    "dev": "./node_modules/.bin/babel src/prlx.js -w -o dist/prlx.js",
    "build": "uglifyjs dist/prlx.js -c -m -r 'recalculate' -o dist/prlx.js"
  },
  "babel": {
    "plugins": [
      "transform-es2015-arrow-functions",
      "transform-es2015-block-scoped-functions",
      "transform-es2015-block-scoping",
      "transform-es2015-parameters",
      "transform-es2015-shorthand-properties",
      "transform-es2015-template-literals"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-plugin-transform-es2015-arrow-functions": "^6.22.0",
    "babel-plugin-transform-es2015-block-scoped-functions": "^6.22.0",
    "babel-plugin-transform-es2015-block-scoping": "^6.23.0",
    "babel-plugin-transform-es2015-parameters": "^6.23.0",
    "babel-plugin-transform-es2015-shorthand-properties": "^6.22.0",
    "babel-plugin-transform-es2015-template-literals": "^6.22.0",
    "uglify-js": "^2.7.5"
  }
}
